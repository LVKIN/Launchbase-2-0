<html>
  <head>
    <title>
      Rocketseat
    </title>
    <link rel="stylesheet" href="styles.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

  </head>
  <body>
    <header>
      <div class="links">
        <a href="/">About</a>
        <a href="/contents">Contents</a>
      </div>
    </header>
    <div class="wrapper">
      <h1 style="color: #50fa7b;">Contents</h1>
    </div>
    <section class="cards">
      
      <div class="card" id="starter">
        <div class="card__image-content">
          <img src="https://skylab.rocketseat.com.br/static/64c237ccff807c054339a62d53b4b402.svg" alt="starter">
        </div>
        <div class="card__content">
          <p>Starter</p>
        </div>
        <div class="card__info">
          <p class="card__modules">2 Phases</p>
          <p class="card__cost">Free</p>
        </div>
      </div>

      <div class="card" id="launchbase">
        <div class="card__image-content">
          <img src="https://skylab.rocketseat.com.br/static/0828532024cb46921a6b5e941f8d788d.svg" alt="launchbase">
        </div>
        <div class="card__content">
          <p>Launchbase</p>
        </div>
        <div class="card__info">
          <p class="card__modules">8 Weeks</p>
          <p class="card__cost">Paid</p>
        </div>
      </div>

      <div class="card" id="gostack">
        <div class="card__image-content">
          <img src="https://skylab.rocketseat.com.br/static/83a178a0653dab1d55e2ed7946465975.svg" alt="go-stack">
        </div>
        <div class="card__content">
          <p>Go Stack!</p>
        </div>
        <div class="card__info">
          <p class="card__modules">8 Weeks</p>
          <p class="card__cost">Paid</p>
        </div>
      </div>
    </section>
    <div class="modal-overlay active">
      <div class="modal">
        <a class="modal-close">
          <i class="material-icons">close</i>
        </a>
        <div class="modal-content">
          <iframe src="" frameborder="0"></iframe>
        </div>
      </div>
    </div>
    <script src="scripts.js"></script>
  </body>
</html>